{
    "name": "rich-id/tour-bundle",
    "description": "A tour bundle for Symfony 6.0",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "RichId",
            "homepage": "https://www.rich-id.fr"
        }
    ],
    "support": {
        "email": "team-devs@rich-id.io",
        "source": "https://github.com/rich-id/tour-bundle",
        "docs": "https://github.com/rich-id/tour-bundle/blob/master/README.md",
        "issues": "https://github.com/rich-id/tour-bundle/issues"
    },
    "require": {
        "php": "^8.1",
        "doctrine/orm": "^2.7 || ^3.0",
        "richcongress/bundle-toolbox": "*",
        "symfony/event-dispatcher": "^6.0 || ^7.0",
        "symfony/http-foundation": "^6.0 || ^7.0",
        "symfony/security-bundle": "^6.0 || ^7.0",
        "symfony/twig-bundle": "^6.0 || ^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "richcongress/test-suite": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "RichId\\TourBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RichId\\TourBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
