<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping>
    <mapped-superclass name="RichId\TourBundle\Model\UserTourPerformed" table="rich_id_user_tour_performed" indexed="true">
        <id name="user" association-key="true" />
        <id name="tour" column="tour" type="string" nullable="true" />

        <many-to-one field="user" target-entity="RichId\TourBundle\Model\UserTourInterface">
            <join-column name="user_id" referenced-column-name="id" nullable="false" />
        </many-to-one>
    </mapped-superclass>
</doctrine-mapping>
